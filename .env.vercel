# Vercel Production Environment Template
# Copy these to your Vercel environment variables

# Database Configuration (Supabase)
DATABASE_URL=postgresql://postgres:[password]@[host]:[port]/[database]?sslmode=require

# Server Configuration
NODE_ENV=production
PORT=3000

# Session Configuration
SESSION_SECRET=your-super-secure-production-session-secret-here

# Authentication
USE_REPLIT_AUTH=false
ISSUER_URL=https://replit.com/oidc
REPL_ID=your-repl-id
REPLIT_DOMAINS=your-production-domain.vercel.app

# Hedera Blockchain Configuration
HEDERA_ACCOUNT_ID=0.0.6554099
HEDERA_PRIVATE_KEY=0xfe9389ffe1467bb16ff67f6c152b5e58c1e3a7c03617daedd3f5ed7de86
HEDERA_NETWORK=testnet

# AI Service Configuration
GEMINI_API_KEY=your-gemini-api-key-here

# CORS Configuration
FRONTEND_URL=https://your-frontend-domain.vercel.app
